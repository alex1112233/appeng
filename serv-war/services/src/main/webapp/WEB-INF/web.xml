<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
  <servlet>
  	<servlet-name>LogServlet</servlet-name>
  	<display-name>LogServlet</display-name>
  	<description></description>
  	<servlet-class>be.post.service.LogServlet</servlet-class>
  </servlet>
  
  
  <servlet>
  	<servlet-name>ImageRenderingServlet</servlet-name>
  	<display-name>ImageRenderingServlet</display-name>
  	<description></description>
  	<servlet-class>be.post.service.ImageRenderingServlet</servlet-class>
  </servlet>
  
 
  
  <servlet>
  	<servlet-name>ResponseSendingServlet</servlet-name>
  	<display-name>ResponseSendingServlet</display-name>
  	<description></description>
  	<servlet-class>be.post.service.ResponseSendingServlet</servlet-class>
  </servlet>

	<servlet-mapping>
		<servlet-name>LogServlet</servlet-name>
		<url-pattern>/LogServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ImageRenderingServlet</servlet-name>
		<url-pattern>/getReq</url-pattern>
	</servlet-mapping>
	
	
	 <servlet-mapping>
		<servlet-name>ResponseSendingServlet</servlet-name>
		<url-pattern>/sendResp</url-pattern>
	</servlet-mapping>
</web-app>
